  "repository": {
    "type": "git",
    "url": "https://github.com/kaisermann/kaisermann.me"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "cross-env": "7.0.2",
    "cssnano": "^4.1.10",
    "eslint": "^7.5.0",
    "eslint-config-kiwi": "^1.3.4",
    "html-minifier": "^4.0.0",
    "postcss": "^7.0.32",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "terser": "^4.8.0",
    "xml2js": "^0.4.23"
  }
}
