{
  "name": "kaisermann.me",
  "version": "2.0.0",
  "author": "Christian Kaisermann",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env ELEVENTY_ENV=development eleventy --serve",
    "build": "cross-env ELEVENTY_ENV=production eleventy",
    "build:dev": "eleventy",
    "build:prod": "run-s build purge-css critical-css minify-html",
    "lint": "eslint --ignore-pattern=\"node_modules\" .",
    "format": "prettier --write \"**/*.{js,json}\"",
    "critical-css": "./scripts/critical-css.js",
    "minify-html": "./scripts/minify-html.js",
    "purge-css": "purgecss -c purgecss.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaisermann/kaisermann.me"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@kiwi/eslint-config": "^1.2.0",
    "@kiwi/prettier-config": "^1.1.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "axios": "^0.21.1",
    "critical": "^2.0.4",
    "cross-env": "7.0.2",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "eslint": "^7.8.1",
    "eslint-config-kiwi": "^1.3.4",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.32",
    "postcss-extend-rule": "^3.0.0",
    "postcss-import": "^12.0.1",
    "postcss-load-config": "^2.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.1",
    "purgecss": "^2.3.0",
    "rollup": "^2.26.11",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-svelte": "^6.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.25.0",
    "svelte-preprocess": "^4.2.1",
    "terser": "^5.3.1",
    "tiny-glob": "^0.2.8",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "js-levenshtein": "^1.1.6"
  }
}
